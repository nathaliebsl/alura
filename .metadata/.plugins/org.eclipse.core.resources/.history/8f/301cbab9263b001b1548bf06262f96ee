
public class TestaIRPF {
	

	    public static void main(String[] args) {

	        double salario = 3300.0;

	        if(salario >= 1900 && salario <= 2800) {
	        	
	        	System.out.println("deduçaão de R$ 142");
	        } if(salario >= 2800.01 && salario <= 3751) {
	        	
	        	System.out.println("dedução de R$ 350);
	        } else (salario >= 3751.01 && salario <= 4664) {
	        	
	        	System.out.println("dedução de R$ 636");
	        	
	        }
	        	
	    }

}
